    "def check(func):\n",
    "   def wrap(arg, k):\n",
    "      try:\n",
    "         func(arg, k)\n",
    "      except ZeroDivisionError:\n",
    "         print('Cannot be divided')\n",
    "   return wrap\n",
    "\n",
    "a = int(input('1-sonni kirit: '))\n",
    "b = int(input('2-sonni kirit: '))\n",
    "\n",
    "@check\n",
    "def div(a, b):\n",
    "   print(a/b)\n",
    "div(a,b)"
